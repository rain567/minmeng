<style type="text/css">
.active-button{margin:0;}
div.center form dl dd.l {width:100px;}
.refresh{cursor:pointer;}

.form{padding:10px}
.form dd.l{float:left;color:#666;width:150px;text-align:right;}
.form dl{clear:both}

#forumname{font-weight:bold}
#forumname *{font-weight:bold}

.footer-buttons{position:fixed;bottom:0;height:30px;width:100%}
.footer-buttons button{float:left;height:30px;font-size:16px;cursor:pointer}
.footer-buttons button:hover{background:#ff6600;color:#fff}
.footer-buttons .save-button{background:#0066cc;color:#fff;border:0;width:50%}
.footer-buttons .close-button{background:#eee;color:#666;border:0;width:50%}

button.f-button{border:1px solid #ccc;line-height:25px;padding-left:10px;background:#fff;
		padding-right:10px;font-size:14px;cursor:pointer;}
button.f-button:hover{border:1px solid #ccc;background:#fff;}

button.f-button.submit{border:1px solid #ff0000;background:#ff6600;color:#fff;}
button.f-button.submit:hover{border:1px solid #ff0000;background:#ff0000;color:#fff;}

button.f-button.reset{border:1px solid #ccc;background:#eee;color:#333}
button.f-button.reset:hover{border:1px solid #999;background:#999;color:#fff}

div.center form dl dd.l{width:100px}
div.center form dl dd{margin-bottom:10px;}
div.center form .text {padding:5px;border:1px solid #ccc}
div.center form dl dd a{color:#999;}
